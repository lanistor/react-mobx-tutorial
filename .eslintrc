{
  "extends": ["rackt", "plugin:react/recommended"],
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "react/wrap-multilines": 2,
    "space-before-function-paren": 0
  },
  "plugins": [ "react", "babel" ]
}
